{
  "_version": "1.76.0",
  "sap.app": {
    "id": "com.demo.b78sapui5",
    "type": "application",
    "i18n": "i18n/i18n.properties",
    "applicationVersion": {
      "version": "0.0.1"
    },
    "title": "{{appTitle}}",
    "description": "{{appDescription}}",
    "resources": "resources.json",
    "sourceTemplate": {
      "id": "@sap/generator-fiori:basic",
      "version": "1.20.0",
      "toolsId": "93091086-dd2a-4baa-a923-195010591eb3"
    },
    "dataSources": {
      "mainService": {
        "uri": "/sap/opu/odata/sap/ZB78_EMP_SRV/",
        "type": "OData",
        "settings": {
          "annotations": [],
          "localUri": "localService/mainService/metadata.xml",
          "odataVersion": "2.0"
        }
      }
    }
  },
  "sap.ui": {
    "technology": "UI5",
    "icons": {
      "icon": "",
      "favIcon": "",
      "phone": "",
      "phone@2": "",
      "tablet": "",
      "tablet@2": ""
    },
    "deviceTypes": {
      "desktop": true,
      "tablet": true,
      "phone": true
    }
  },
  "sap.ui5": {
    "flexEnabled": false,
    "dependencies": {
      "minUI5Version": "1.143.2",
      "libs": {
        "sap.m": {},
        "sap.ui.core": {}
      }
    },
    "contentDensities": {
      "compact": true,
      "cozy": true
    },
    "models": {
      "i18n": {
        "type": "sap.ui.model.resource.ResourceModel",
        "settings": {
          "bundleName": "com.demo.b78sapui5.i18n.i18n"
        }
      },
      "oModel": {
        "dataSource": "mainService",
        "preload": true,
        "settings": {}
      },
      "empModel": {
        "type": "sap.ui.model.json.JSONModel"
      },
      "lookupModel": {
        "type": "sap.ui.model.json.JSONModel",
        "uri": "model/lookupdata.json"
      }
    },
    "resources": {
      "css": [
        {
          "uri": "css/style.css"
        }
      ]
    },
    "routing": {
      "config": {
        "routerClass": "sap.m.routing.Router",
        "controlAggregation": "pages",
        "controlId": "app",
        "transition": "slide",
        "type": "View",
        "viewType": "XML",
        "path": "com.demo.b78sapui5.view",
        "async": true,
        "viewPath": "com.demo.b78sapui5.view"
      },
      "routes": [
        {
          "name": "RouteView1",
          "pattern": "",
          "target": [
            "TargetView1"
          ]
        },
        {
          "name": "RouteView2",
          "pattern": "secondview/{key}",
          "target": [
            "TargetView2"
          ]
        },
        {
          "name": "RouteView3",
          "pattern": "thirdview",
          "target": [
            "TargetView3"
          ]
        },
        {
          "name": "RouteView4",
          "pattern": "fourthview/{key}",
          "target": [
            "TargetView4"
          ]
        }
      ],
      "targets": {
        "TargetView1": {
          "id": "View1",
          "name": "View1"
        },
        "TargetView2": {
          "id": "View2",
          "name": "View2"
        },
        "TargetView3": {
          "id": "View3",
          "name": "View3"
        },
         "TargetView4": {
          "id": "View4",
          "name": "View4"
        }
      }
    },
    "rootView": {
      "viewName": "com.demo.b78sapui5.view.App",
      "type": "XML",
      "id": "App",
      "async": true
    }
  }
}